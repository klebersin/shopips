<%include layout/_header%>
<div class="container">
    <div class="row">
        <div class="col s12">
            <table>
                <thead>
                    <tr>
                        <th>Nombre</th>
                        <th>Apellidos</th>
                        <th>Usuario</th>
                        <th>Tipo de usuario</th>
                        <th>Opciones</th>
                    </tr>
                </thead>
                <tbody>
                    <%for(var i=0;i<data.length;i++){%>
                    <tr>
                        <td><%=data[i].nombre%></td>
                        <td><%=data[i].apellidos%></td>
                        <td><%=data[i].usuario%></td>
                        <td><%=data[i].tipo%></td>
                        <%if(data[i].tipo=='normal'&& data[i].nombre!=kleber){%>
                        <td><a href="/hacerAdmin/<%=data[i].id%>" class="btn">Hacer administrador</a></td>
                        <%}else{%>
                            <td><a href="/hacerNormal/<%=data[i].id%>" class="btn">Hacer Normal</a></td>
                        <%}%>
                    </tr>
                    <%}%>
                </tbody>
            </table>
        </div>
    </div>
</div>
<%include layout/_footer%>